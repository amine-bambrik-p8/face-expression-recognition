{
    "exp_name": "fer2013_exp_deepbn",
    "agent": "GenericAgent",
    "mode": "test",
    "model":"LeNetVGGDeepBN2",
    "cuda": true,
    "gpu_device": 0,
    "seed": 1,
    "valid_size":0.1,
  
    "batch_size": 16,
    "test_batch_size": 64,
  
    "learning_rate": 0.004,
    "loss_function":"NLLLoss",
  
    "max_epoch": 1,
    "log_interval": 10,
  
    "validate_every": 2,
    "optimizer":"simple_sgd_optim",
    "test_transform":"gray_scale",
    "train_transform":"gray1_rhflip_rrotate_ccrop40",

    "checkpoint_file": "checkpoint.pth.tar",
    "data_loader": "ImageFERDataLoader",
    "data_loader_workers": 2,
    "pin_memory": true,
    "train_datafolder": "./data/FER/train",
    "test_datafolder": "./data/FER/test"

}