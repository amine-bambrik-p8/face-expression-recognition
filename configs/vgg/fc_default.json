    "test_transform":"gray_scale",
    "valid_transform":"gray_scale",
    "train_transform":"gray_scale",
    "train_datafolder": "./data/FER/train",
    "test_datafolder": "./data/FER/test",
    "valid_datafolder": "./data/FER/validation",
    
    "data_loader_workers": 2,
    "pin_memory": true,

    "cuda": true,
    "gpu_device": 0,
