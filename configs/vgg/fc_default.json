{
    "label":"vgg",
    "exp_name": "default",
    "description":"vgg with fc decoder",
    "agent": "GenericAgent",

    "model":"GoodFellowV3",
    "loss_function":"NLLLoss",
    "do_save":false,
    "decoder":"BasicDecoder",
    "in_channels":1,
    "encoder_dropout":0.1,
    "encoder_fn":"ReLU",
    "encoder_fn_params":[],
    "encoder_batch_norm":true,
    "encoder_channels":[64,64,128],
