{
    "label":"vgg",
    "exp_name": "test_final",
    "description":"vgg with avg decoder",
    "agent": "GenericAgentLRDecay",

    "model":"GoodFellowV3",
    "loss_function":"NLLLoss",
    "decoder":"AvgDecoder",
    "in_channels":1,
    "encoder_dropout":0.0,
