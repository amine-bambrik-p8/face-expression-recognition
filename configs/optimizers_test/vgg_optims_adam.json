{
    "label":"adam",
    "exp_name": "vgg_optims",
    "description":"VGG optims test logging adam accuracy and loss",
    "agent": "GenericAgent",

    "model":"GoodFellowV3Down",
    "loss_function":"NLLLoss",
    "do_save":false,
    "decoder":"NetInNetDecoder",
    "in_channels":1,
    "encoder_dropout":0.1,
    "encoder_fn":"LeakyReLU",
    "encoder_fn_params":[0.01],
    "encoder_batch_norm":true,
    "encoder_channels":[64,64,128],
    "encoder_depths":[2,2,2],
    "n_classes":7,
    "decoder_channels":[128,512,512],
    "decoder_fn":"LeakyReLU",
    "decoder_fn_params":[0.01],
    "decoder_dropout":0.4,
    "decoder_batch_norm":true,
    "class_fn":"LogSoftmax",
    "optimizer":"adam",

    "learning_rate": 0.0002,
    "betas":[
        0.9,
        0.99
    ],
    "weight_decay":0,
    "max_epoch": 50,
    "log_interval": 10,
    "validate_every": 1,
    
    
    "data_loader": "ImageFERDataLoader",
    "batch_size": 64,
    "test_transform":"gray_scale_histo_equal_face",
    "valid_transform":"gray_scale_histo_equal_face",
    "train_transform":"gray1_rhflip_histo_equal_rrotate_face",
    "train_datafolder": "./data/FER/train",
    "test_datafolder": "./data/FER/test",
    "valid_datafolder": "./data/FER/validation",
    
    "data_loader_workers": 2,
    "pin_memory": true,

    "cuda": true,
    "gpu_device": 0,
    "seed": 1,
    "checkpoint_file": "checkpoint.pth.tar"
}