{
    "label":"vgg",
    "exp_name": "vgg",
    "description":"vgg with avg decoder",
    "agent": "GenericAgent",

    "model":"GoodFellowV3",
    "loss_function":"NLLLoss",
    "decoder":"NetInNetDecoder",
    "in_channels":1,
    "encoder_dropout":0.1,
    "encoder_fn":"LeakyReLU",
    "encoder_fn_params":[0.1],
    "encoder_batch_norm":true,
    "encoder_channels":[64,64,128],
    "encoder_depths":[2,2,2],
    "n_classes":7,
    "decoder_channels":[128,1024,1024],
    "decoder_fn":"LeakyReLU",
    "decoder_fn_params":[0.1],
    "decoder_dropout":0.4,
    "decoder_batch_norm":true,
    "class_fn":"LogSoftmax",
    "optimizer":"adam",

    "learning_rate": 0.0005,
    "lr_scheduler_factor":0.1,
    "lr_scheduler_mode":"min",
    "min_lr":0.00001,
    "betas":[
        0.9,
        0.99
    ],
    "weight_decay":0.0000001,
    "max_epoch": 30,
    "log_interval": 10,
    "validate_every": 1, 
    
    "data_loader": "ImageFERDataLoader",
    "batch_size": 128,
    "test_transform":"gray_scale_histo_equal",
    "valid_transform":"gray_scale_histo_equal",
    "train_transform":"gray1_rhflip_histo_equal",
    "train_datafolder": "./data/FER/train",
    "test_datafolder": "./data/FER/test",
    "valid_datafolder": "./data/FER/validation",
    
    "data_loader_workers": 2,
    "pin_memory": true,

    "cuda": true,
    "gpu_device": 0,
    "seed": 1,
    "checkpoint_file": "checkpoint.pth.tar"
}